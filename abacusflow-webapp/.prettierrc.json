{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": false,
  "printWidth": 100,
  "endOfLine": "auto",
  "useTabs": false,
  "tabWidth": 2,
  "arrowParens": "always",
  "trailingComma": "none",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "vue"
      }
    }
  ]
}
